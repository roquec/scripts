{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "palette": {
        "time-foreground": "#a9a9a9",
        "session-foreground": "#E36464",
        "path-foreground": "#56B6C2",
        "dotnet-foreground": "#ac99ea",
        "git-foreground": "#DDB15F",
        "git-branch-status-foreground": "#a3a5e7",
        "git-changes-foreground": "#e7b5a3",
        "git-staging-foreground": "#c7e7a3",
        "prompt-foreground": "#cccccc",
        "ellapsed-foreground": "#a9a9a9",
        "status-ok-foreground": "#68db7f",
        "status-error-foreground": "#E36464"
    },
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "foreground": "p:time-foreground",
                    "properties": {
                        "time_format": "15:04"
                    },
                    "style": "plain",
                    "template": "[{{ .CurrentDate | date .Format }}]",
                    "type": "time"
                },
                {
                    "foreground": "p:session-foreground",
                    "style": "plain",
                    "template": " \uf2bd {{ .UserName }}",
                    "type": "session"
                },
                {
                    "foreground": "p:path-foreground",
                    "properties": {
                        "style": "folder"
                    },
                    "style": "plain",
                    "template": " \ue5ff {{ .Path }}",
                    "type": "path"
                },
                {
                    "type": "dotnet",
                    "style": "plain",
                    "foreground": "p:dotnet-foreground",
                    "template": " \udb81\ude10 {{ .Full }}"
                },
                {
                    "foreground": "p:git-foreground",
                    "properties": {
                        "branch_icon": "",
                        "fetch_status": true,
                        "fetch_upstream_icon": true
                    },
                    "style": "plain",
                    "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if or .BranchStatus .Working.Changed .Staging.Changed }}(<p:git-branch-status-foreground>{{if .BranchStatus }}{{ .BranchStatus }}{{ end }}</><p:git-changes-foreground>{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}</><p:git-staging-foreground>{{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }}</>){{ end }}",
                    "type": "git"
                },
                {
                    "foreground": "p:prompt-foreground",
                    "style": "plain",
                    "template": " \u276f",
                    "type": "text"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "segments": [
                {
                    "foreground": "p:ellapsed-foreground",
                    "properties": {
                        "style": "roundrock",
                        "threshold": 0
                    },
                    "style": "plain",
                    "template": "~{{ .FormattedMs }}",
                    "type": "executiontime"
                },
                {
                    "foreground": "p:status-ok-foreground",
                    "properties": {
                        "always_enabled": true
                    },
                    "style": "plain",
                    "template": "{{ if eq .Code 0 }}üíö{{ else }}<p:status-error-foreground>üíî</>{{end}}",
                    "type": "status"
                }
            ],
            "type": "rprompt"
        }
    ],
    "final_space": true,
    "version": 2,
    "auto_upgrade": true,
    "disable_notice": true
}